# Configuration de l'environnement pour le chatbot frontend
# Les variables doivent commencer par REACT_APP_ pour être accessibles dans React

# URL de base de l'API backend
REACT_APP_API_BASE_URL=http://localhost:8080

# Endpoint pour les requêtes de chat
REACT_APP_CHAT_ENDPOINT=/api/chat/query

# Configuration de l'application
REACT_APP_NAME=Chatbot Assistant
REACT_APP_VERSION=1.0.0

# Configuration de session
REACT_APP_SESSION_PREFIX=session_

# Configuration de développement
REACT_APP_DEBUG=true